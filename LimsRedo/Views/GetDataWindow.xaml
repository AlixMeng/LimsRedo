<Window x:Class="LimsRedo.GetDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LimsRedo"
        mc:Ignorable="d"
        Title="GetDataWindow" Height="300" Width="525">
    <Grid>
        <ComboBox x:Name="SearchTypeCombo" HorizontalAlignment="Left" Margin="29,22,0,0" VerticalAlignment="Top" Width="103"/>
        <TextBox x:Name="SearchValueCombo" HorizontalAlignment="Left" Height="23" Margin="132,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="273"/>
        <ListBox x:Name="ResultListBox" HorizontalAlignment="Left" Height="198" Margin="29,61,0,0" VerticalAlignment="Top" Width="460"/>
        <Button x:Name="SearchBtn" Content="Search" HorizontalAlignment="Left" Margin="408,21,0,0" VerticalAlignment="Top" Width="84" Height="24" Click="SearchBtn_Click"/>

    </Grid>
</Window>
